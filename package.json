{
  "name": "@fabrigeas/ts-boilerplate",
  "version": "2.2.0",
  "description": "A boilerplte that installs usefull stuff line sass prettier eslint husky jest and other utilities to you noe project",
  "main": "install.sh",
  "scripts": {
    "test": "sh test.sh",
    "tsc": "tsc --project tsconfig.json",
    "lint": "eslint './**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint --fix './**/*.{js,jsx,ts,tsx,json}'",
    "pretty": "prettier --write . --config ./.prettierrc.json --loglevel silent",
    "prepare": "husky install",
    "minor": "npm version minor && npm publish",
    "patch": "npm version patch && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push --all --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fabrigeas/ts-boilerplate.git"
  },
  "keywords": [
    "prettier",
    "eslint",
    "husky",
    "precommit hook",
    "git-commit-message-linter",
    "sass",
    "jest",
    "gitlab-ci",
    "sass",
    "grunt"
  ],
  "bin": {
    "main": "install.sh"
  },
  "author": "Fabrice Feugang Kemegni",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fabrigeas/ts-boilerplate/issues"
  },
  "files": [
    "files/*"
  ],
  "homepage": "https://github.com/fabrigeas/ts-boilerplate#readme",
  "dependencies": {
    "@fabrigeas/ts-boilerplate": "^2.0.0",
    "ts-boilerplate": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "2.8.3",
    "ts-node": "^10.9.1",
    "tsli": "^0.0.1-security",
    "typescript": "^4.9.5"
  }
}
